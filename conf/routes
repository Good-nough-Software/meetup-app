# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET         /                          controllers.LoginController.renderViewLogin
GET 	    /home		       controllers.HomeController.index
GET 	    /results		       controllers.HomeController.results

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)

GET         /viewNewUser              controllers.newUserController.renderViewNewUser


GET         /viewResetPassword        controllers.resetPasswordController.renderViewResetPassword

GET         /logout                   controllers.LoginController.logout()

GET         /test                     controllers.LoginController.test()

GET         /debug                    controllers.debugController.main


POST        /login                    controllers.LoginController.login()

POST        /createNewUser            controllers.newUserController.createNewUser()

POST        /resetPassword            controllers.resetPasswordController.resetPassword()
